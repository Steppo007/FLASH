{
  "name": "@flash/ui-components",
  "version": "3.5.1",
  "type": "commonjs",
  "repository": {
    "url": "ssh://git@pd.bitbucket.syskron.com/kop_app_shell/flash.ui-components.git"
  },
  "engines": {
    "node": "^22",
    "npm": "^10"
  },
  "scripts": {
    "prepare": "husky",
    "export": "ts-node ./scripts/exporter.ts",
    "package": "ts-node ./scripts/packager.ts",
    "colordiff": "ts-node ./scripts/colorDiffGenerator.ts",
    "gen:version": "ts-node ./scripts/versionExtractor.ts",
    "gen:svg": "ts-node ./scripts/svgComponentGenerator.ts",
    "gen:tailwind": "ts-node ./scripts/tailwindColorsGenerator.ts",
    "new:comp": "ts-node ./scripts/componentGenerator.ts",
    "sync:docs": "npm run prettier && ts-node ./scripts/docsSynchronizer.ts",
    "gen": "npm run gen:version && npm run gen:svg && npm run gen:tailwind",
    "postinstall": "npm run gen",
    "build": "npm run build:cl && npm run build:sb",
    "build:cl": "npm run gen && rimraf ./build/components && tsc -p tsconfig.complib.json && npm run package",
    "build:sb": "npm run gen && npx storybook build --output-dir build/storybook",
    "ci:prettier": "npx prettier . --check",
    "sb": "npx storybook dev --no-open -p 6006",
    "serve:sb": "vite preview --outDir ./build/storybook --port 6006",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier . --write",
    "release": "npx semantic-release",
    "test": "./tests/ci-runner.sh",
    "test-mac": "./tests/ci-runner-mac.sh"
  },
  "peerDependencies": {
    "primereact": "^10.9.7",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@fontsource/roboto": "^5.1.0",
    "@playwright/test": "^1.56.1",
    "@prettier/plugin-xml": "^3.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@storybook/addon-designs": "^10.0.2",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-links": "^9.1.5",
    "@storybook/react-vite": "^9.1.5",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^22.9.0",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vueless/storybook-dark-mode": "^9.0.8",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-storybook": "^9.1.5",
    "glob": "^11.0.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "object-hash": "^3.0.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "primereact": "^10.9.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.0",
    "storybook": "^9.1.5",
    "tailwindcss": "^4.1.13",
    "tar": "^7.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^7.1.5",
    "vite-plugin-svgr": "^4.3.0"
  },
  "lint-staged": {
    "{src/**,.}/*.{js,jsx,ts,tsx}": "npx --no-install eslint --fix --report-unused-disable-directives --max-warnings 0",
    "**/*": "npx --no-install prettier --write"
  },
  "dependencies": {
    "@uidotdev/usehooks": "^2.4.1",
    "react-transition-group": "^4.4.5"
  }
}
